{
    "name": "ajgl/doctrine-dbal-pgsql-types",
    "type": "library",
    "description": "Custom PostgreSQL types",
    "keywords": [
        "dbal",
        "database",
        "posgresql",
        "psql"
    ],
    "homepage": "https://github.com/ajgarlag/AjglDoctrineDbalPgsqlTypes",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio J. Garc√≠a Lagar",
            "email": "aj@garcialagar.es",
            "homepage": "http://aj.garcialagar.es",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.3 || ^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.58 || ^11.5 || ^12.3",
        "friendsofphp/php-cs-fixer": "^3.88.2"
    },
    "autoload": {
        "psr-4": {
            "Ajgl\\Doctrine\\DBAL\\Types\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ajgl\\Doctrine\\DBAL\\Types\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "0.4.x-dev"
        }
    },
    "config": {
        "preferred-install": {
            "doctrine/dbal": "source"
        }
    },
    "scripts": {
        "test": [
            "@php vendor/bin/phpunit --colors=always"
        ]
    }
}
