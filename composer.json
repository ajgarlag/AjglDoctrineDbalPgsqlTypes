{
    "name": "ajgl/doctrine-dbal-pgsql-types",
    "type": "library",
    "description": "Custom PostgreSQL types",
    "keywords": ["dbal", "database", "posgresql", "psql"],
    "homepage": "https://github.com/ajgarlag/AjglDoctrineDbalPgsqlTypes",
    "license": "MIT",
    "authors": [
      {
        "name": "Antonio J. Garc√≠a Lagar",
        "email": "aj@garcialagar.es",
        "homepage": "http://aj.garcialagar.es",
        "role": "developer"
      }
    ],
    "require": {
        "doctrine/dbal": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4.1"
    },
    "autoload": {
        "psr-4": { "Ajgl\\Doctrine\\DBAL\\Types\\": "src" }
    },
    "autoload-dev": {
	"psr-4": { "Ajgl\\Doctrine\\DBAL\\Types\\Tests\\": "tests" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    },
    "config": {
        "preferred-install": {
            "doctrine/dbal": "source"
        }
    },
    "scripts": {
        "phpunit": "vendor/bin/phpunit --colors=always",
        "test": [
            "@phpunit"
        ]
    }
}
